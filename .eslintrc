{
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "max-len": [
      "error",
      150
    ],
    "linebreak-style": 0,
    "no-console": 0,
    "indent": [
      "error",
      2
    ]
  }
}